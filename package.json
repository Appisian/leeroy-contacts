{
  "name": "nuxt-leeroy-contact",
  "version": "0.1.0-0",
  "description": "My breathtaking Nuxt.js project",
  "author": "appisian",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel dev:nuxt theo:onchange theo",
    "dev:nuxt": "nuxt",
    "build": "npm run theo && nuxt build",
    "start": "cross-env NODE_ENV=production nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "theo": "theo ./tokens/index.yml --transform web --format styl,json,raw.json --dest ./assets/tokens",
    "theo:onchange": "onchange \"./tokens/*.yml\" -- npm run theo"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/pwa": "^3.0.0-0",
    "cross-env": "^5.2.1",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "lodash": "^4.17.15",
    "nib": "^1.1.2",
    "nuxt": "^3.4.3",
    "rupture": "^0.7.1",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.0",
    "prettier": "^1.16.4",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "theo": "^8.1.4",
    "vue-webpack-loaders": "^1.0.8",
    "webpack": "^4.41.2"
  }
}
